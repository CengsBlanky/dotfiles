alias l="ls -l --color=always --group-directories-first"
alias ll="ls -lAh --color=always --group-directories-first"
alias cls="clear"
alias ..="cd .."
alias gst="git status"
alias gdf="git diff"
alias gcm="git commit -a"
alias gps="git push"
alias gpl="git pull"
alias vims="sudo $EDITOR"
alias python="python3"
